ME = .
RANDFILE = $ENV::HOME/.rnd</code>

####################################################################
[ req ]
default_bits = 2048
default_keyfile = private.key
distinguished_name = server_distinguished_name
req_extensions = server_req_extensions
string_mask = utf8only

####################################################################
[ server_distinguished_name ]
countryName = Country Name (2 letter code)
countryName_default = US

stateOrProvinceName = State or Province Name (full name)
stateOrProvinceName_default = MA

localityName = Locality Name (eg, city)
localityName_default = Boston

organizationName = Organization Name (eg, company)
organizationName_default = Test LLC

commonName = Common Name (e.g. server FQDN or YOUR name)
commonName_default = ec2-54-186-217-17.us-west-2.compute.amazonaws.com

emailAddress = Email Address
emailAddress_default = contact@ec2-54-186-217-17.us-west-2.compute.amazonaws.com

####################################################################
[ server_req_extensions ]

subjectKeyIdentifier = hash
basicConstraints = CA:FALSE
keyUsage = digitalSignature, keyEncipherment
subjectAltName = @alternate_names
nsComment = "OpenSSL Generated Certificate"

####################################################################
[ alternate_names ]

DNS.1 = ec2-54-186-217-17.us-west-2.compute.amazonaws.com
DNS.2 = www.ec2-54-186-217-17.us-west-2.compute.amazonaws.com
DNS.3 = mail.ec2-54-186-217-17.us-west-2.compute.amazonaws.com
DNS.4 = ftp.ec2-54-186-217-17.us-west-2.compute.amazonaws.com

